{
  "name": "fp-tx",
  "version": "0.0.1",
  "description": "fp-ts forever",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "prettier": "prettier \"./{src,test,examples,scripts}/**/*.ts\"",
    "prettier:fix": "prettier --write \"./{src,test,examples,scripts}/**/*.ts\"",
    "test": "pnpm run jest",
    "jest": "jest",
    "clean": "rimraf ./dist",
    "prebuild": "bun run clean",
    "preconstruct": "preconstruct",
    "build": "tsup",
    "dpdm": "dpdm --warning=false --tree=false --exit-code circular:1 -T src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacob-alford/fp-tx.git"
  },
  "author": "Jacob Alford <jacob.alford@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/fp-tx/issues"
  },
  "homepage": "https://github.com/jacob-alford/fp-tx",
  "devDependencies": {
    "@preconstruct/cli": "^2.8.1",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "benchmark": "2.1.4",
    "bun-types": "^1.0.12",
    "docs-ts": "^0.7.0",
    "eslint": "^8.54.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fast-check": "^3.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.0"
  },
  "tags": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "keywords": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "dependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "tsup": "^7.3.0"
  }
}
